.rounded-standard { -moz-border-radius:4px; -webkit-border-radius:4px; border-radius:4px; }
  
.dboards-btn{
  color:#000;
  font-weight: bold;
  text-decoration:none;
  text-align:center;
  display:inline-block;
  *display:inline; /* IE6/7 fix */
