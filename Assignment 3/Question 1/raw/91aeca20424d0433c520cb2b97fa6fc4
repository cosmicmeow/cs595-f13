				<style type="text/css">
					#clockBG_Kia {
						float: right;
						width: 355px;
						height: 43px;
						margin-top: -32px;
					}
					
					#clock_Kia {
						margin-left: -50px;
						margin-top: -30px;
						color: #FFF;
						font-family: Arial;
						font-size: 22px;
						font-weight: bold;
						letter-spacing: 0;
						word-spacing: 12px;
					}
					#cntdwn_Kia {
						color: #FFF !important;
						position: relative;
						top: 3px;
						left:3px;
						word-spacing: 15px;
						font-size: 19px;
						display:none;
					}
					.nbaSkin.kia {
						height: 43px !important;
						background-image: url('http://i.cdn.turner.com/nba/nba/.element/img/3.0/sect/home/nba_clockBG_kia.png') !important;
						background-position-x: 1px !important;
						background-repeat: no-repeat !important;
					}
					.nbaSkin.kia_nowPlaying {
						height: 43px !important;
						background-color: #000;
						background-image: url('http://i.cdn.turner.com/nba/nba/.element/img/3.0/sect/home/nba_clockBG_kia_nowPlaying.png') !important;
						background-position-x: 1px !important;
						background-repeat: no-repeat !important;
					}
				</style>
				
				<script language="JavaScript">
						
					TargetDate = "10/29/2013 7:00 PM UTC-0400";
					BackColor = "none";
					ForeColor = "#FFF";
					CountActive = true;
					CountStepper = -1;
					LeadingZero = true;
					DisplayFormat = "%%D%% %%H%% %%M%% %%S%%";
					FinishMessage = "Now Playing&nbsp;";
				</script>
		
				<script language="JavaScript">
				
					/* determine what to display, e.g. 12d, 8h, 6m, 4s */
					function calcTime(secs, num1, num2) {
						kia_seconds = ((Math.floor(secs / num1)) % num2).toString();
						if (LeadingZero && kia_seconds.length < 2) {
							kia_seconds = "0" + kia_seconds;
						}
						return "<b>" + kia_seconds + "</b>";
					}
					
					function countKia(secs) {
						var kia = $("#cntdwn_Kia");
						/* is the countdown complete? */
						if (secs < 0) {
							setTimeout(deleteKia, 100);
							return;
						}
						DisplayStr = DisplayFormat.replace(/%%D%%/g, calcTime(secs, 86400, 100000));
						DisplayStr = DisplayStr.replace(/%%H%%/g, calcTime(secs, 3600, 24));
						DisplayStr = DisplayStr.replace(/%%M%%/g, calcTime(secs, 60, 60));
						DisplayStr = DisplayStr.replace(/%%S%%/g, calcTime(secs, 1, 60));
						
						/* update the clock */
						kia.html(DisplayStr);
						
						if (CountActive) {
							setTimeout("countKia(" + (secs + CountStepper) + ")", SetTimeOutPeriod);
						}
					}
					
					function deleteKia(){
						/* remove the Kia Countdown Timer and replace the BG Skin */
						$('#body_Kia').remove();
						/* remove the Kia Countdown BG Skin */
						$(".nbaSkin").removeClass("kia");
						/* add the new Now Playing BG Skin */
						$(".nbaSkin").addClass("kia_nowPlaying");
						/* add in the TNT OT clock */
						$('#body_Tnt').load('/.element/ssi/sect/3.0/home/countdownClockTntOT.html'); 
					}
					
					function createKia() {
						var dthen = new Date(TargetDate);
						var dnow = new Date();
						if (CountStepper > 0) {
							ddiff = new Date(dnow - dthen);
						} else {
							ddiff = new Date(dthen - dnow);
						}
						/* determine how many seconds are left in the countdown */
						gsecs = Math.floor(ddiff.valueOf() / 1000);
						
						/* executed upon page load */
						if(gsecs < 0) {
							/* the countdown is already finished */
							$(".nbaSkin").addClass("kia_nowPlaying");
							/* add in the TNT OT clock */
							$('#body_Tnt').load('/.element/ssi/sect/3.0/home/countdownClockTntOT.html'); 
						} else {
							$("#body_Kia").append('<div id="clockBG_Kia"><img src="http://i.cdn.turner.com/nba/nba/.element/img/3.0/sect/home/nba_clockBG_kia_right.png"><div id="clock_Kia"><span id="cntdwn_Kia"></span></div></div>');
							/* add the Kia Countdown BG Skin */
							$(".nbaSkin").addClass("kia");
							/* start counting down */
							countKia(gsecs);
						}
					}
					
					/* error handling */
					if (typeof (BackColor) == "undefined") { BackColor = "none"; }
					if (typeof (ForeColor) == "undefined") { ForeColor = "#FFF"; }
					if (typeof (TargetDate) == "undefined") { TargetDate = "10/29/2013 7:00 PM UTC-0400"; }
					if (typeof (DisplayFormat) == "undefined") { DisplayFormat = "%%D%% %%H%% %%M%% %%S%%"; }
					if (typeof (CountActive) == "undefined") { CountActive = true; }
					if (typeof (FinishMessage) == "undefined") { FinishMessage = "Now Playing&nbsp;"; }
					if (typeof (CountStepper) != "number") { CountStepper = -1; }
					if (typeof (LeadingZero) == "undefined") { LeadingZero = true; }
					CountStepper = Math.ceil(CountStepper);
					if (CountStepper == 0) { CountActive = false; }
					var SetTimeOutPeriod = (Math.abs(CountStepper) - 1) * 1000 + 990;
					
					/* create the HTML */
					setTimeout(createKia, 100);
					
					$(window).load(function() {
  						$("#cntdwn_Kia").show();
					});
					
				</script>
		
		
				<div id="body_Kia"></div>
				<div id="body_Tnt"></div>
				
   
		
    			
