var fifteenDays = 1000*60*60*24*30;
var expiress = new Date((new Date()).valueOf() + fifteenDays);
document.cookie = "visited=true;expiress=" + expiress.toUTCString();
$.colorbox({width:"350px", inline:true, href:"#subscribe"});
}
});
</script>
