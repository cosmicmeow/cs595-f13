<script>
var limit=3;
var now =1;
var page_width=jQuery('.lastest_headlines_page').width()+2;

jQuery(document).ready(function() {
var flag=true;

jQuery('#mysliderright').click(function(event) {
  if(flag && now<limit){
	now++;
	flag=false;
  	jQuery('#myslider').animate({
   	 left: '-='+page_width
  	}, 600, function() {
		if(now == 2) jQuery("#pageNumber").html('51 - 100');
		if(now == 3) jQuery("#pageNumber").html('101 - 150');
		flag=true;
		if(now==limit){ jQuery('#mysliderright').addClass('disable');}
		jQuery('#mysliderleft').removeClass('disable');
    // Animation complete.
  	});
  }
});

jQuery('#mysliderleft').click(function(event) {
  if(flag && now>1){
	now--;
  	flag=false;
  	jQuery('#myslider').animate({
    	left: '+='+page_width
  	  }, 600, function() {
		if(now == 2) jQuery("#pageNumber").html('51 - 100');
		if(now == 1) jQuery("#pageNumber").html('1 - 50');
    	flag=true;
		if(now==1){ jQuery('#mysliderleft').addClass('disable');}
		jQuery('#mysliderright').removeClass('disable');
 	  });
   }
});
});
</script>	

