	<div class="itemHeader">
	  	  <h1 class="componentheading" >
	  	Man injured in Ensay house fire		
			  </h1>
	  	          <div class="small_red item_date">
        	Fri 27 Sep 2013        </div>
      	  
				
  </div>


  <!-- Plugins: AfterDisplayTitle -->
    
  <!-- K2 Plugins: K2AfterDisplayTitle -->
  


  <div class="itemTools">
		<div id="k2_author_date">
