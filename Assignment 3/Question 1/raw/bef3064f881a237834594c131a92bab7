<script type="text/javascript">

    $ib(document).ready(function(){
        var cOffset = 0;
        var cLength = 38;

        var bRB = $ib('.slideshowGallery .bottomRightButton');
        var bLB = $ib('.slideshowGallery .bottomLeftButton');

        if(cOffset == 0) {
            bLB.hide();
        }

        if(cOffset == cLength - 1)  {
            bRB.hide();
        }

        bRB.click(function() {
            $ib('.slideshowGallery .imageViewer a.navNextNormal').click();
        });

        bLB.click(function() {
            $ib('.slideshowGallery .imageViewer a.navPrevNormal').click();
        });

        $ib("#items22135818 .1 img").addClass("active");

        // set initial nav state in the carousel
        var carouselParams = {
            navPrev: "#thmbNavLeft22135818",
            navNext: "#thmbNavRight22135818",
            numVisibleSlides: 6,
            numSlides: 39,
            scrollable: "#scrollable22135818"
        };

        $ib("#scrollable22135818").scrollable({
            onSeek: function() {
                ibsys.htvGallery.setThumbnailIndicatorPosition(carouselParams);
            }
        });

        ibsys.htvGallery.setCarouselNavState(carouselParams);
        ibsys.htvGallery.setThumbnailIndicatorPosition(carouselParams);

        // bind click handling to carousel show/hide toggles
        ibsys.htvGallery.bindCarouselToggleClicks("#hideCarouselToggle", "#showCarouselToggle", "#carousel22135818");

        // bind mouseover/click handling on carousel buttons
        ibsys.htvGallery.bindCarouselNavHandlers(carouselParams);

        // set ad refresh intervals
        var adParams = {
            pageAdRefreshInterval : "1"
        };
        ibsys.htvGallery.setAdRefreshIntervals(adParams);

        ibsys.htvGallery.setSlideChangeCounter( { slideChangeCounter : 1 } );

        
        try{
            // bind click handling to all the anchors in the carousel
            var comscoreParams = {
                csEnabled: true,
                csParams: {c1:"2",c2:"6034975",c3:"",c4:"",c5:"",c6:"",c15:""}
            };

            $ib("#items22135818 a").each(function(idx) {
                var params = {
                    offset: idx,
                    galleryId: 22135818,
                    numVisibleSlides: 6,
                    numSlides: 39,
                    comscoreParams: comscoreParams
                };
                ibsys.htvGallery.bindClick($ib(this), params);
            });

        } catch (err) {
            ibsys.log("gallery: " + err);
        }
        
    });
</script>
