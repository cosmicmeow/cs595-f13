	<!-- Newsbeat --> 
	<script type='text/javascript'>
	    var _sf_async_config={};
	    var metaTags = document.getElementsByTagName("meta")
	    /** CONFIGURATION START **/
	    _sf_async_config.uid = 19989;
	    _sf_async_config.domain = 'vulture.com';
	    _sf_async_config.sections = 'Vulture';  //CHANGE THIS
	    _sf_async_config.authors = '';
	    _sf_async_config.useCanonical = true; 
	    
	    for(i=0; i<metaTags.length; i++){ 
	        if(metaTags[i].name=="author"){
	             _sf_async_config.authors = metaTags[i].content;
	        } else if(metaTags[i].name=="content.hierarchy"){
	            var tmp = metaTags[i].content.split(":");
	            _sf_async_config.sections += "," + tmp[0];
				if(tmp[1]=="Topics"){
					_sf_async_config.sections = "Vulture,Topics";
				} else if(_sf_async_config.sections == "Vulture,Clickables"){
	                _sf_async_config.sections = "Vulture,Other";
	            } else if(_sf_async_config.sections == "Vulture,Main"){
	                _sf_async_config.sections = "Vulture,Main Index"
	            }
	        }
	    }
	
	    /** CONFIGURATION END **/
	    (function(){
	      function loadChartbeat() {
	        window._sf_endpt=(new Date()).getTime();
	        var e = document.createElement('script');
	        e.setAttribute('language', 'javascript');
	        e.setAttribute('type', 'text/javascript');
	        e.setAttribute('src',
	           (('https:' == document.location.protocol) ? 'https://a248.e.akamai.net/chartbeat.download.akamai.com/102508/' : 'http://static.chartbeat.com/') +
	           'js/chartbeat.js');
	        document.body.appendChild(e);
	      }
	      var oldonload = window.onload;
	      window.onload = (typeof window.onload != 'function') ?
	         loadChartbeat : function() { oldonload(); loadChartbeat(); };
	    })();
	</script>

