<script type="text/javascript">

    function openWindow(url) {
      width = window.screen.width;
      height = window.screen.height;
      leftWindow = 0;
      topWindow = 0;
      options = 'toolbar=0,locationbar=0,status=0,width='+width+',height='+height+',top='+topWindow+',left='+leftWindow;
      window.open(url, "cManager", options);
    }

  </script>
