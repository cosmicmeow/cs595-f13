		$(function () {
			var force_js = true;
			$("#CU3ER").cu3er({
				vars: {
					xml_location : '/templates/cmanager/showcase/CU3ER-config.xml?unchache=1335362846',
					folder_images:"/templates/cmanager/showcase/images",
					folder_fonts:"/templates/cmanager/showcase/fonts",
					swf_location: 'CU3ER.swf',
					<!-- js_location: '/js/jquery.cu3er.player.old.js', -->
					width: "940",
					height: "300",
					folder_images:"/templates/cmanager/showcase/images",
					flash_version : "10.0.0",
					display_js3d_first: true,
					
					xml_encoded: "%3Cdata%3E%3Clicence%3E%3C!%5BCDATA%5B!icw.KRyQk2MtG1i5'ACI%7BB%7BI)fS'18Q%5D%5D%3E%3C%2Flicence%3E%3C%2Fdata%3E"
				},
				params: {
				  wmode : 'transparent'  		},
				attributes: {
					id:'CU3ER',
					name:'CU3ER'
				},
				  onSliderInit : function() {
				
					switch (CU3ER['CU3ER'].displayVersion) {
						case "Flash":
							$("#info-txt").html("You are now seeing <strong>CU3ER Flash</strong> version, since you do not have CSS 3D capable browser.<br/>Try Chrome, Safari or Firefox 11+ to see Javascript 3D version of CU3ER.");
						break;
						case "JS-3D":
							$("#info-txt").html("You are now seeing <strong>CU3ER Javascript 3D</strong> version. Yes, we know. It looks awesome.");
						break;
						case "JS-Slicing":
							$("#info-txt").html("You are now seeing <strong>CU3ER Javascript Slicing</strong> version, best CU3ER Javascript version your browser can display.<br/>Try Chrome, Safari or Firefox 11+ to see Javascript 3D version of CU3ER.");
						break;
						case "JS-Simple":
							$("#info-txt").html("You are now seeing <strong>CU3ER Javascript Simple</strong> version, best CU3ER Javascript version your browser can display. <br/>Try Chrome, Safari or Firefox 11+ to see Javascript 3D version of CU3ER.");
						break;
					} 
				  }
			});

		});

		
				
				
			

			</script>

		
			
			
			
