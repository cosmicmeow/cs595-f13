l_facebookLinkChannel = '/pages/This-is-Money/310555847252';
l_twitterVia = 'ThisIsMoney';
l_facebookText = 'This is Money';
} else {
l_twitterLinkChannel = currentChannelTwitterFollow || 'MailOnline';	
l_facebookLinkChannel = 'DailyMail';	
l_twitterVia = twitterVia;
l_facebookText = 'MailOnline';
}
function goToFB()
{	
var s15 = DM.siteCatalyst.getInstance();
s15.linkTrackVars = s.linkTrackVars = "events,prop4,prop34,eVar4,eVar11,eVar32,eVar50";
s15.linkTrackEvents = s15.events = s.linkTrackEvents = s.events = "event23";
s15.prop4 = s15.eVar4 = s.prop4 = s.eVar4 = "social_share";
s15.eVar11 = s.eVar11 = "D=pageName";
s15.prop34 = s15.eVar32 = s.prop34 = s.eVar32 = "facebook_like_non-native";
s15.eVar50 = s.eVar50 = "D=Referer";
s.tl(true, "o", "social_share - facebook_like_non-native");
s15.tl(true, "o", "social_share - facebook_like_non-native");
window.open("https://www.facebook.com/" + l_facebookLinkChannel);
}
function followOnTwitter() {
var s15 = DM.siteCatalyst.getInstance();
s15.linkTrackVars = s.linkTrackVars = "events,prop4,prop34,eVar4,eVar11,eVar32,eVar50";
s15.linkTrackEvents = s15.events = s.linkTrackEvents = s.events = "event23";
s15.prop4 = s15.eVar4 = s.prop4 = s.eVar4 = "social_share";
s15.eVar11 = s.eVar11 = "D=pageName";
s15.prop34 = s15.eVar32 = s.prop34 = s.eVar32 = "twitter_share_non-native";
s15.eVar50 = s.eVar50 = "D=Referer";
s.tl(true, "o", "social_share - twitter_share_non-native");
s15.tl(true, "o", "social_share - twitter_share_non-native");
var width = 575,
height = 400,
left = (window.outerWidth - width) / 2,
top = (window.outerHeight - height) / 2,
url = this.href,
opts = 'resizable=1,scrollbars=1,status=1' +
',width=' + width +
',height=' + height +
',top=' + top +
',left=' + left;
var newwindow = window.open("http://twitter.com/intent/user?screen_name=" + l_twitterLinkChannel + "&related=" + l_twitterVia, "", opts);
return false;
}
Ext.select('.twpuffbutton p').update('<b>Follow</b> <br/>@' + l_twitterLinkChannel);
Ext.select('.fbpuffbutton p').update('<b>Like</b> <br/>' + l_facebookText);
</script>
