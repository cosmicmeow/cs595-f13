(function ($) {
    "use strict";
    var ipadMessageKey = 'iPadMessage',
        buttonValue;
    // popup for desktop (debug and development)
    //if ($.jStorage.get(ipadMessageKey) !== (undefined || 'iPadMessageShown')) {
    // popup for ipad only
	if (($.jStorage.get(ipadMessageKey) !== (undefined || 'iPadMessageShown')) && (navigator.userAgent.match(/(iPad)/g))) {
        $('<div id="ipad-detect" class="group-layout-1 ipad-detect"><div class="module-header"><h3 class="heading"><a href="#" class="button-close"></a></h3></div><div class="module-footer"><h3 class="heading"><a href="https://itunes.apple.com/au/app/news.com.au-for-ipad/id687614497" class="button-download"></a></h3></div></div>').prependTo('#page').animate({
            opacity: 1
        }, {
            duration: 700,
            queue: false
        }).slideDown('slow');
    }
    $('#ipad-detect .module-header a, #ipad-detect .module-footer a').click(function () {
	   $('#ipad-detect').slideUp('slow');
        $.jStorage.set(ipadMessageKey, 'iPadMessageShown');
        buttonValue = $(this).attr('class');
        utag.link({
            ev_cat: "ipad-download-message",
            ev_action: "promo-popup",
            ev_label: buttonValue
        });
        //return false;
    });
}(ndm.jQuery));
