});
function social_button_clicked(site,placement) {
var shareValue = site+"_share_article-"+placement;
var s15 = DM.siteCatalyst.getInstance();
s15.linkTrackVars = s.linkTrackVars = "events,prop4,prop34,eVar4,eVar11,eVar32,eVar50,eVar51";
s15.linkTrackEvents = s15.events = s.linkTrackEvents = s.events = "event23";
