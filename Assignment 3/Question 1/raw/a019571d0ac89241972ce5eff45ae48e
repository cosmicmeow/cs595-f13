<script type='text/javascript'>
 //<![CDATA[
imgr = new Array();
imgr[0] = "http://i43.tinypic.com/15x05f4.gif";
showRandomImg = true;

aBold = true;

summaryPost = 120; 
summaryTitle = 25; 

numposts = 0; 
numposts1 = 1; 
numposts2 = 2; 
numposts3 = 3; 
numposts4 = 4; 
numposts5 = 5; 
