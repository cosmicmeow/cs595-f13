<script language="JavaScript" type="text/javascript">
  // reload page
  function reload(){
    if(isSafari){
      location.replace(location.href.replace(/#/,'?')); // a hack, but it works.
    } else if(isFirefox){
      parent.location.replace(location.href);
    } else {
      parent.location.reload(true);
    }
  }
