      if (!window.registeredBloggerCallbacks) {
        window.registeredBloggerCallbacks = true;

        

        
        gadgets.rpc.register('requestReload', function() {
          document.location.reload();
        });

        
        gadgets.rpc.register('requestSignOut', function(siteId) {
          
          google.friendconnect.container.openSocialSiteId = siteId;
          google.friendconnect.requestSignOut();
        });
      }
    </script>
<script type="text/javascript">
    
    function registerGetBlogUrls() {
      gadgets.rpc.register('getBlogUrls', function() {
        var holder = {};
        
          
            
            
              holder.currentPost = "http://www.blogger.com/feeds/2680139916738735125/posts/default/822147171712740865";
            
            
            
              holder.currentComments = "http://www.blogger.com/feeds/2680139916738735125/822147171712740865/comments/default";
            
            holder.currentPostUrl = "";
            holder.currentPostId = 822147171712740865
          
          
          
            holder.postFeed = "http://www.blogger.com/feeds/2680139916738735125/posts/default";
          
          
          
            holder.commentFeed = "http://www.blogger.com/feeds/2680139916738735125/comments/default";
          
          holder.currentBlogUrl = "http://saga-crepusculoap.blogspot.com/";
          holder.currentBlogId = "2680139916738735125";
        
        return holder;
      });
    }
  </script>
<script type="text/javascript">
  if (!window.registeredCommonBloggerCallbacks) {
    window.registeredCommonBloggerCallbacks = true;

