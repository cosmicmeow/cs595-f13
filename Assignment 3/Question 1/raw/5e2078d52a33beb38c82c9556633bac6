    // Event handlers for the list...
    $(function() {

        $("#blogarticle_autoplay_next").click(function(e) {
            if(DEBUG_ENABLED) console.log("blogarticle_autoplay_next button clicked.");
            e.preventDefault();
            $("#blogarticle_autoplay_next").fadeOut(200).fadeIn(550);

            var nextArticleUrl = "http://www.snipblog.com/a/2013-09-26/androidcentral/speedtest-net-app-updated-with-new-interface-maps-feature";
            window.location = nextArticleUrl; 

        });

        $("#blogarticle_autoplay_prev").click(function(e) {
            if(DEBUG_ENABLED) console.log("blogarticle_autoplay_prev button clicked.");
            e.preventDefault();
            $("#blogarticle_autoplay_prev").fadeOut(200).fadeIn(550);

            var prevArticleUrl = "http://www.snipblog.com/a/2013-09-26/ciocom/microsoft-vows-greater-financial-transparency-with-new-reporting-format";
            window.location = prevArticleUrl; 

        });
    	
    });
