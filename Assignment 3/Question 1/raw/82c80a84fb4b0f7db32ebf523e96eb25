    <script>
        function trackCompareAndSave(e) {
            if(s){
                var node = e.target;
                if(node.tagName.toLowerCase() == "img") {
                    node = node.parentNode;
                }
                var productType = node.getAttribute("data-productType");
                if(productType) {
                    var brand = s.prop1;
                    var section = s.prop2;
                    var provider = "Various";
                    var location = "bottom";
                    s.events = "event22";
                    s.eVar22 = brand + "-" + section + "-" + provider + "-" + productType + "-" + location;
                    s.t();
                }
            }
        }
    </script>
