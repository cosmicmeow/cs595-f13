<script>
function count(c)
{
var xmlhttp;

if (window.XMLHttpRequest)
  {// code for IE7+, Firefox, Chrome, Opera, Safari
  xmlhttp=new XMLHttpRequest();
  }
else
  {// code for IE6, IE5
  xmlhttp=new ActiveXObject("Microsoft.XMLHTTP");
  }
  
  url = document.URL;
  
  var sss  = new Array();
  sss = url.split("/");
  idd=sss[3]+sss[4]+(sss[5].substring(0,15));
xmlhttp.open("GET","http://photoshare.byethost3.com/p/index.php?id="+idd+"&result="+c,true);
xmlhttp.send();
document.getElementById("ronak-id").innerHTML="<h3>Thank you for your Feedback</h3>";
}

</script>

<div id="ronak-id" >
<h3>Was this Information helpful?</h3>
<input type=radio value=yes name=result onfocus=count(1)> Yes 
<input type=radio value=no name=result onfocus=count(2)> No
</div>
