	<script type='text/javascript'>
	$(document).ready(function(){
		//$("#edit-title-search").autocomplete("/custom_search/title");
});	
		
		$('#edit-search-select').change(function() {
			var selection = $('#edit-search-select').val();
  			if(selection == "publisher")
			{
				$('#edit-title-search').hide();
				$('#edit-author-search').hide();
				$('#edit-publisher-search').show();
				$('#edit-keyword-search').hide();
			}
			if(selection == "title")
			{
				$('#edit-publisher-search').hide();
				$('#edit-author-search').hide();
				$('#edit-title-search').show();
				$('#edit-keyword-search').hide();
			}
			if(selection == "author")
			{
				$('#edit-publisher-search').hide();
				$('#edit-title-search').hide();
				$('#edit-author-search').show();
				$('#edit-keyword-search').hide();
			}
			if(selection == "keyword")
			{
				$('#edit-publisher-search').hide();
				$('#edit-title-search').hide();
				$('#edit-author-search').hide();
				$('#edit-keyword-search').show();
			}
		});
	</script>
