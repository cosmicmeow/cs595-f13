                <script type="text/javascript">
                    /* when changing the select option, load the URL to filter by the date  */
                    $j('#dateFilter select').change(function() {
                        var authorVal = $j('#dateSelect').val();
                        var url = 'http://scn.sap.com/community/java/blog/'
                        window.location = url + authorVal;
                    });
                    /* once loaded, set the select to the currently filtered date */
                    $j(document).ready(function() {
                        var currentUrl = window.location.href;
                        var authorValue = currentUrl.split("blog/");
                        var value = authorValue[1];
                        $j('#dateSelect').val(value).attr('selected', 'selected');
                    });
                </script>
        



