	 <script type="text/javascript">	 
		function submitNewsletterSubscriptionToaster(){		
			var postData=$("#newsletterModuleFormToaster").serialize();
	 		var email = $.trim($('#newsletterModuleFormToaster [name="email"]').val());					
			if(!checkEmailValid(email)){
				alert("Please enter a valid email address.");
				return false;
			} 			
			else if(postData.indexOf("prodid")==-1){
				alert("Please Select at least one Newsletter.");
				return false;
			}
			var requestOptions={
						url: '/newsletters/nl_module_processor',
						data: postData,
						success: function(data,textStatus){
								$("#toasterNewsletter").fadeOut("slow",function(){$(this).html(data).fadeIn("slow");
									})
							}
				};
			$.ajax(requestOptions);
		}
	</script>	
	     
