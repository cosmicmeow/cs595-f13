		<script>

			(function($){

				// check for overlay suppression flag
				if ( $( 'body' ).data( 'dt_suppress_overlays' ) )
					return;

				var is_suppressed = false;

				if ( DT.getUrlParameter( 'dt_show_fb_prompt' ) ){

					DT.Lightbox.show_dialog( 'facebook_like', ajaxurl + '?action=dt_facebook_like_dialog', 590, 280 );
					return;

				}

				if ( is_suppressed )
					return;

				if ( DT.ObjectCache.getItem( 'dt_fb_prompt' ) == '1' )
					return;

				if ( $.cookie( 'dt_fb_prompt' ) !== undefined )
					return;

				$( 'body' ).bind( 'dtsso_facebook_ready', function( evt ){

					FB.getLoginStatus( function( response ){

						if ( response.status == 'connected' ){

							DT.Logger.log("Checking if fb user has liked our page");

							FB.api( '/me', function( user ){

								var uid = user.id;

								var fql = "SELECT uid FROM page_fan WHERE page_id = 8522591132 AND uid = " + uid;

								FB.api({
									method: 'fql.query',
									query: fql
								}, function( response2 ){

									//don't check again for another 24 hrs
									DT.ObjectCache.setItem( 'dt_fb_prompt', '1', 60 * 60 * 24 );
									$.cookie( 'dt_fb_prompt', '1', { expires: 1, path: '/' } );

									//if we got something that means this person has already liked the page
									if ( response2.length ){

										//They already like us. Our work here is done.

									} else {

										DT.Lightbox.show_dialog( 'facebook_like', ajaxurl + '?action=dt_facebook_like_dialog', 590, 280 );

									}

								});

							});

						} else { //user has not authorized fb or is not logged in

							DT.Logger.log("Cannot determine if fb user has liked our page");

							//don't check again for another 30 days
							DT.ObjectCache.setItem( 'dt_fb_prompt', '1', 60 * 60 * 24 * 30 );
							$.cookie( 'dt_fb_prompt', '1', { expires: 30, path: '/' } );

							DT.Lightbox.show_dialog( 'facebook_like', ajaxurl + '?action=dt_facebook_like_dialog', 590, 280 );

						}

					});

				});

			})(jQuery);

