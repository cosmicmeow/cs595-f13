<script>
    sugestao = {
        init: function () {
           var userAgent = navigator.userAgent.toLowerCase();
           if(userAgent.indexOf('iphone')!=-1){
               document.getElementById('help').setAttribute('id','helpMenuIOS');
            }
            if (typeof(jQuery) != "undefined") {
                if ($('.footer').html() != null) {
                    $('<a id="footer-open-sugestao" href="http://fb.submarino.com.br/portal/sugestao/" class="popIframe" data-height="460" data-width="520">Qual produto você gostaria de ver no Submarino?</a>').appendTo('form.searchFooter-a');
                } else {
                    setTimeout("sugestao.init();", 1000);
                }
            } else {
                setTimeout("sugestao.init();", 1000);
            }
        }
    }
    sugestao.init();

    function _GET(name) {
        var url = window.location.search.replace("?", "");
        var itens = url.split("&");

        for (n in itens) {
            if (itens[n].match(name)) {
                return decodeURIComponent(itens[n].replace(name + "=", ""));
            }
        }
        return null;
    }
    function setCookie(cookie_name, cookie_value, expire_in_days) {
        if (cookie_value == null)return;

        var cookie_expire = "";

        if (expire_in_days != null) {
            var expire = new Date();
            expire.setTime(expire.getTime() + 1000 * 60 * 60 * 24 * parseInt(expire_in_days));
            cookie_expire = "; expires=" + expire.toGMTString();
        }

        document.cookie = escape(cookie_name) + "=" + escape(cookie_value) + cookie_expire + ";domain=.submarino.com.br;path=/";
    }
    setCookie("b2wChave", _GET('chave'), 1);
</script>
<!-- End Sugestão de Cliente -->

