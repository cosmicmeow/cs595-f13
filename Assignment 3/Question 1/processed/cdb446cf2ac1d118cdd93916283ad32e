<script type="text/javascript">
	function abreLightBox(ob, type){
		/*
			Can be set type to 'image', 'ajax', 'iframe', 'swf' or 'inline';
			Optional: 
				- data-width
				- data-height
			example:
				<a href="http://www.submarino.com.br" onclick="return abreFancyBox(this, 'iframe');" data-width="680" data-height="400" >Abre lightbox</a>
		*/
		var fbUrl 	= $(ob).attr('href');
		var nw		= $(ob).data('width') || 680;
		var nh		= $(ob).data("height") || 400;
		$.fancybox({
			width			: nw,
			height			: nh,
			type			: type || 'ajax',
			autoDimensions	: false,
			href			: fbUrl,
			onComplete		: function(){
				$.fancybox.resize();
			}
		});
		return false;
	}
</script>
