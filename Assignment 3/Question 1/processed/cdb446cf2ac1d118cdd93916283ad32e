	<script type="text/javascript">
onReady('jQuery', function(){
	var loaded			= false;
	$(document).ready(function(){
		loaded		= true;
	})
	var getEl		= function(){
		return $('#readRatings');
	};
	var getURLParameter = function (name) {
		return decodeURI(
			(RegExp(name + '=' + '(.+?)(&|$)').exec(location.search)||[,null])[1]
		);
	}
	var moveScroll		= function(mili){
		try{
			var hash        = getURLParameter('scroll');
			if(hash == 'avaliacao'){
				$('html, body').animate({
					scrollTop: getEl().offset().top - 30
				}, (mili || 2000));
			}
		}catch(e){}
	};
	var timer = setInterval(function(){
		if(getEl().text().replace(/\n/g,"").replace(/\r/g,"").replace(/\t/g,"").replace(/ /g,"").length && loaded){
			clearInterval(timer);
			getEl().removeClass('loading');
			moveScroll(500);
		}
	}, 100);
	getEl().addClass('loading');
	moveScroll();
});
</script>
