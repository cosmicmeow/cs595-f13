	<script>
	/**
	 * This rogersClubLoginCheck() function is a JSON-P callback that modifies the above
	 * login form to correspond to the user's logged-in state. For this reason, the JS
	 * is located here so that it can be run as soon as possible. Note, however, that the
	 * script is loaded asynchronously so that the page load will not be blocked.
	 */
	function rogersClubLoginCheck( info ){
		var container = document.getElementById('loyalty-club');

		// Not authenticated
		if( !info || !info.auth || info.auth.error || !info.auth.member ){
			container.className += " not-authenticated";
		}
		// Authenticated
		else {
			container.className += " authenticated";
			var member = info.auth.member;
			document.getElementById('loyalty-club-user-name').appendChild(document.createTextNode(member.firstname));
		}
		container.className = container.className.replace(/\bbefore-auth-check\b/, '');

		// Cleanup
		var script = document.getElementById('rogersClubLoginCheckScript');
		if(script){
			script.parentNode.removeChild(script);
		}
	}
	// Dynamically insert the script tag asynchronously so we don't block the page from loading
	(function(){
		var matches = document.cookie.match(/md5id=(\S+?)\s*(;|$)/);
		if(!matches){
			rogersClubLoginCheck(false);
		}
		else {
			var script = document.createElement('script');
			script.id = 'rogersClubLoginCheckScript';
			script.async = true;
			script.src = "http:\/\/jackaholicsclub.1031jackfm.ca\/chtt\/nonmembers\/login\/verify.jsp?jsonp=rogersClubLoginCheck&md5id=" + matches[1];
			document.getElementsByTagName('head')[0].appendChild(script);
		}
	})();
	</script>
