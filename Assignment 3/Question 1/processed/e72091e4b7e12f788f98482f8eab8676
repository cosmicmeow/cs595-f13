$(document).ready(function() {
	// Toggle Panel
	$('.toggle').bind('click', function(){
		if( $(this).parents('div.collapsiblebox').hasClass('close') )
			$(this).parents('div.collapsiblebox').removeClass('close');
		else
			$(this).parents('div.collapsiblebox').addClass('close');
		if( $('div.collapsiblebox').hasClass('close') )
			$('#expand').removeAttr('checked');
		else
			$('#expand').attr('checked', 'checked');
		return false;
	});
	$('#expand').bind('click', function(){
		if( $('#expand').attr('checked') )
			$('div.collapsiblebox').removeClass('close');
		else
			$('div.collapsiblebox').addClass('close');
	});
});
//--></script>
