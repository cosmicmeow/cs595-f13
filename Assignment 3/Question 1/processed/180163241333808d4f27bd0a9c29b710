			jQuery(document).ready(function() {
			function pop_tracking()
			{
				jQuery.post('http://dineronuevoconstante.net/?plugin=plm&mode=tracking', { id: '3', ip: '128.82.4.182' });
			}

					jQuery("#bgPopup").data("state", 0);
					plmCenterPopup("#popup-optin");
