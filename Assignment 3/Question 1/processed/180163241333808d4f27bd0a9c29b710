					if( e.keyCode==27 ) {
						plmDisablePopup('#popup-optin');
					}
				});

			
				jQuery('#ar-fields').find('form').submit(function(){
					if ( jQuery(this).find('.plm-name-field').val() == 'Tu Nombre Aquí' ) jQuery(this).find('.plm-name-field').attr('value', '');
					var field_name = jQuery(this).find('.plm-required');
					
					if ( field_name.length ) {
						if ( field_name.val().length < 2 ) {
							field_name.attr('value', '');
							field_name.focus();
							return false;
						}
					}

					var field_email = jQuery(this).find('.plm-required-email');
					var reg = /^([\w-]+(?:\.[\w-]+)*)@((?:[\w-]+\.)*\w[\w-]{0,66})\.([a-z]{2,6}(?:\.[a-z]{2})?)$/i;
					if ( !reg.test(field_email.val()) ) {
						field_email.attr('value', '');
						field_email.focus();
						return false;
					}

					pop_tracking();

					jQuery('#ar-fields').hide();	
					jQuery('#thanks-msg').show();

									});

			
			
			});
