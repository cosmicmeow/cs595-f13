<script type="text/javascript">
      if (!window.registeredBloggerCallbacks) {
        window.registeredBloggerCallbacks = true;

        

        
        gadgets.rpc.register('requestReload', function() {
          document.location.reload();
        });

        
        gadgets.rpc.register('requestSignOut', function(siteId) {
          
          google.friendconnect.container.openSocialSiteId = siteId;
          google.friendconnect.requestSignOut();
        });
      }
    </script>
<script type="text/javascript">
    
    function registerGetBlogUrls() {
      gadgets.rpc.register('getBlogUrls', function() {
        var holder = {};
        
          
            
            
              holder.currentPost = "http://www.blogger.com/feeds/6604738941064687776/posts/default/7537361425024833020";
            
            
            
              holder.currentComments = "http://www.blogger.com/feeds/6604738941064687776/7537361425024833020/comments/default";
            
            holder.currentPostUrl = "";
            holder.currentPostId = 7537361425024833020
          
          
          
            holder.postFeed = "http://www.blogger.com/feeds/6604738941064687776/posts/default";
          
          
          
            holder.commentFeed = "http://www.blogger.com/feeds/6604738941064687776/comments/default";
          
          holder.currentBlogUrl = "http://www.vivaveltoro.com/";
          holder.currentBlogId = "6604738941064687776";
        
        return holder;
      });
    }
  </script>
