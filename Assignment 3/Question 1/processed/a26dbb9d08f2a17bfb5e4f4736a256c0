    <script>
    $(function() {
      parseRSS('http://blog.ziprecruiter.com/feed/?cat=11516189', function(feed) {
        var $container = $('#blogFeed');
        $container.html('');
        var entryLength = 1;
        if (feed.entries.length < entryLength) {
          entryLength = feed.entries.length;
        }
        for (var i =0; i < entryLength; i++) {
          var entry = feed.entries[i];
          entry.publishedDate = entry.publishedDate.substr(0, entry.publishedDate.length-15);
          $container.append(ich.blog_entry_template(entry));
        }
      });
    });
    </script>
