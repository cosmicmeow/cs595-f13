<script type="text/javascript">

function MasVistos(data) {
	var totalVideos = data.feed.openSearch$itemsPerPage.$t;
	
	var feed = data.feed;
	var entries = feed.entry || [];
	
	var html =  ['<div class="videos_destacados">'];   
	for (var i = 0; i < totalVideos; i++) {  
		
		var entry = entries[i]; 
		var idVideos = entry.id.$t.slice(-11)
		var tituloVideos = entry.title.$t;
		var thumbnailUrl = entries[i].media$group.media$thumbnail[0].url;  
		html.push('<div class="vd link_gris">', 
			'<div class="thumb_video"><a href="/veracruz/player?id=',idVideos,'"><img src="',thumbnailUrl,'"></a></div>',
				'<div class="titulo_video">',
					'<div class="vd_titulo"><a href="/veracruz/player?id=',idVideos,'">',tituloVideos,'</a></div>',
				'</div>',
			'</div>',
			'<div><hr></div>');       
		
	
	}
	html.push('</div>');
	document.getElementById('videosMasVistos').innerHTML = html.join('');   
	
}

</script>

<div class="miniTitulos">VIDEOS DESTACADOS <img src="http://media.televisaregional.com/designimages/flecha_naranja_mini.png"></div>

