<script>


jQuery( document ).ready( function() {
	stages = jQuery('#home-stage > div');
	stages.css('display' , 'none');
	stages.css('opacity' , '0');
	stages.css('filter' , 'alpha(opacity=0)');
	stages.find('div').css('bottom' , '-75px');
	
				
		
		
	jQuery.simpleWeather({
		location: 'Veracruz,Veracruz,MEX',
		unit: 'c',
		success: function(weather) {
			var html = '<div class="ciudad">' + weather.city + '</div>';
			html += '<div class="temp-icon"><div id="hdr-climar"><img src="' + weather.thumbnail + '"></div><div class="temp"><div>' + weather.temp + '&deg' + weather.units.temp + '</div><div>&nbsp;</div></div></div>'; 
			html += '<div class="min-max"><span>Máx.&nbsp;' + weather.high + '&deg;&nbsp;</span> / <span>Min.&nbsp;' + weather.low + '&deg;&nbsp;</span></div>';
			jQuery("#clima").html(html);
			},
		error: function(error) {
			jQuery("#clima").html('');
			}
		});
	});
</script>
