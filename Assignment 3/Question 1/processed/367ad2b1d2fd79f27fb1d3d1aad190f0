function frameDim() {
    this.width = 0;
    this.height = 0;
}
var videoDim = videoDim || new frameDim();
    videoDim.width = 610;
    videoDim.height = 344;

</script>

    
        <!-- START: Video Embed Content Widget -->
        <div class="video-widget-container" style="width: 610; height: 344;padding:0px;margin:0px;display:inline;">
                        <!-- START: Flash Player -->
                <div id="IGNPlayerContainer" style="width: 610; height: 344; background: #000000; display: block;"></div>
        <script type="text/javascript">
            var swfVersionStr = "10.1.0";
            var xiSwfUrlStr = "http://oystatic.ignimgs.com/src/core/swf/expressInstall.swf";
            var videoUrl = "http://www.ign.com/videos/2013/08/19/the-last-of-us-whats-the-dlc";
            var autoplay = "";
            var automute = "";
            var urlParts;

            if(autoplay == "1" || autoplay == "true") {
                urlParts = videoUrl.split("?");
                videoUrl += (urlParts.length > 1) ? "&qs_autoplay=true" : "?qs_autoplay=true";
            }

            if(automute == "1" || automute == "true") {
                urlParts = videoUrl.split("?");
                videoUrl += (urlParts.length > 1) ? "&qs_automute=true" : "?qs_automute=true";
            }

            var attributes = {};
                            attributes.id = "IGNPlayer";
                attributes["class"] = "ign-videoplayer";
                        var flashvars = {};
                flashvars.cacheBusting = true;
                flashvars.url = videoUrl;
            var params = {};
                params.quality = "high";
                params.bgcolor = "#000000";
                params.allowscriptaccess = "always";
                params.allowfullscreen = "true";
                params.wmode = "opaque";

            var url = "http://oystatic.ignimgs.com/src/core/swf/IGNPlayer.swf";
            if(flashvars.cacheBusting) {
                url += "?version=3.20130929.01";
            }

            swfobject.embedSWF(url, "IGNPlayerContainer", videoDim.width, videoDim.height, swfVersionStr, xiSwfUrlStr, flashvars, params, attributes);

        </script>
                <!-- END Flash Player -->
                
        
    </div>
        <!-- END: Video Embed Content Widget -->
    </div>
