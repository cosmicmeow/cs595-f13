<script type="text/javascript">
  
  function mostrarLoginCasper(){
  	domain = document.domain;
  
  	client_id = 'eltiempo';
  	if (domain == 'www.motor.com.co' || domain == 'motor.com.co') client_id = 'motor';
  	if (domain == 'www.revistacredencial.com.co' || domain == 'revistacredencial.com.co') client_id = 'credencial';
  
  	if(document.getElementById('sb-title'))
		document.getElementById('sb-title').style.visibility='hidden';
	if(document.getElementById('sb-body-text'))
		document.getElementById('sb-body-text').style.visibility='hidden';
  	if(document.getElementById('sb-info'))
		document.getElementById('sb-info').style.visibility='hidden';
  	Shadowbox.open({
	  player: 'iframe',
	  content: 'https://seg.eltiempo.com/autenticacion/authentication/login/?response_type=code&client_id=' + client_id + '&state=1706333574',
	  width:655,
	  height:287,
	  options: {
		handleOversize: 'none',
		onClose: function(){
		  document.getElementById('sb-title').style.visibility='visible';
  		  document.getElementById('sb-body-text').style.visibility='visible';
  		  document.getElementById('sb-info').style.visibility='visible';
		}
	  }
	});
	return false;
}	
   
</script>

