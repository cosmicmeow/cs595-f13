    var _sf_async_config={};
    /** CONFIGURATION START **/
    _sf_async_config.uid = 50648;
    _sf_async_config.domain = 'eltiempo.com';
    _sf_async_config.useCanonical = true;
    _sf_async_config.sections = "Fotografía";  //CHANGE THIS
