<script type="text/javascript">

sas_loadHandler = function(f) {
	if(f.id == 12359 && !f.hasAd){
		divPauta2 = document.getElementById("sas_"+f.id);
		if(divPauta2 != null){
			padre2 = divPauta2.parentNode;
		}
		if(typeof padre2 != "undefined"){  
			padre3 = padre2.parentNode;
		}
		padre3.removeChild(padre2);	
	}
	if(f.id == 12066 && !f.hasAd){
		divPauta = document.getElementById("sas_"+f.id);
		if(divPauta != null){
			padre = divPauta.parentNode;
		}
		if(typeof padre != "undefined"){  
			padre1 = padre.parentNode;
		}
		padre1.removeChild(padre);	
	}
}
</script>


