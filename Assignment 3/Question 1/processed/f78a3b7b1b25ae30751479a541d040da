			<script src='http://gaia.adage.com/scripts/plugins/jquery.counter-1.0.js' type='text/javascript'></script>
<script type="text/javascript">

//from load_comment.js
var thisComment;

function CommentController(articleId) {
	this.articleId = (articleId) ? articleId : -1;
	this.container = $('#adage_comment');
	this.messager = this.container + '_msg';
	this.loadingMessage = '<div style="margin: 40px auto; width: 200px;"><img src="http://gaia.adage.com/images/global/ajax-indicator-036.gif" width="24" height="24" alt="" style="vertical-align: middle; margin: 0 auto;" /> Loading comments...<\/div>';

	this.output = function(msg) {
		this.container.html(msg);
	}
	this.message = function(msg, err) {
		style = (err) ? 'red' : 'green';
		document.setClass(this.messager, style);
		document.getElementById(this.messager).innerHTML = msg;
	}
	this.begin = function() {
		this.output(this.loadingMessage);
		thisComment=this;

        $.ajax({
            url: "/ajax/get_comments.php?article_id=" + this.articleId,
            global: false,
            type: "GET",
			dataType: 'html',
            success: function(data){
				thisComment.output(data);

				// Loading comments sorting and diaplaying from cookies
				if (typeof(commentsResort) !== 'undefined') {
					commentsResort.loadFromCookies();
				}
			}
        });
	}
}
//end load_comment.js

var article_id = 244395;
$(document).ready(function() {

	var comments = new CommentController(article_id);
	if (1) {
		comments.begin();
	}

	//init fb like buttons
	$('.fb_like').onefblike();
	$('.fb_like-hor').onefblikehor();
});

var current_clicked = 0;

function showHide(shID, commentCount, commentID) {
	if (document.getElementById(shID)) {
		if (document.getElementById(shID+'-show').style.display != 'none') {
			$('#comment_form0').show();
			document.getElementById(shID+'-show').style.display = 'none';
			showRecaptcha('recaptcha_div_0', 'submit_0', 'recaptcha_required_0', 'white');
			document.getElementById('reply_link_' + commentCount).innerHTML= 'Reply';
			current_clicked = 0;	
		}
		else {
			$('#comment_form0').hide();
			document.getElementById(shID+'-show').style.display = 'block';
			document.getElementById('reply_link_' + commentCount).innerHTML= 'Close';

			if (current_clicked != 0 ) {
            	document.getElementById('reply_link_' + current_clicked).innerHTML= 'Reply';
	            document.getElementById('comment-form-' + current_clicked + '-show').style.display = 'none';
			}
			current_clicked = commentCount;
            div =  'recaptcha_div_' + commentCount.replace(/\./g,'_');
            sub =  'submit_ ' + commentCount.replace('.','_');
            req =  'recaptcha_required_' + commentCount.replace('.','_');
            showRecaptcha(div, sub, req, 'white');
        }
    } 
};

function showRecaptcha(element, submitButton, recaptchaButton, themeName, capType) {
	/* Recap */

	if (capType == "Re") {
		Recaptcha.destroy();
		Recaptcha.create("6Ld31QsAAAAAAMuF12ILR27jEOfNViuMyx9kum__", element, {
			theme: themeName,
			tabindex: 0
		});
	}

	/* Solve */
	if (capType == "SolveMedia") {
		ACPuzzle.destroy();
		ACPuzzle.create('3UXl-3oBd4d1To.-ZVZIg2XtXcp5Ikwk', element, { lang: 'en', size: 'standard', theme: 'white', size: '300x150' });
	}

    $("." + element).show();
    $(".contact_submit").hide();
    $(".recaptcha_required").show();
    $("#"+recaptchaButton).hide();
    $("#"+submitButton).show();
};

var voterObj = null;
var comment_container_ids = new Array();
var Voter = {

    // HEAD CALL FOR COMMENT VOTING
    vote_comment : function(vote_value, comment_id, user_id) {
        // PRELIMINARY VALIDATION
        if(!isNumeric(vote_value) || !isNumeric(comment_id) || !isNumeric(user_id)){ 
            return;
        }
        if(vote_value > 1 || vote_value < 0){
            return;
        }

        // SHOW THE PROCESSING DIV
        this.show_processing(comment_id);

        voterObj = this;

        // AJAX CALL
        new AJAX.FileRequest(
            '/ajax/comment_vote.php',  
            this.result,
            null,
            'post',
            'comment_id='+comment_id+'&vote_value='+vote_value+'&user_id='+user_id
        );
    },

    vote_spam : function(vote_value, comment_id, user_id) {
        // PRELIMINARY VALIDATION
        if(!isNumeric(vote_value) || !isNumeric(comment_id) || !isNumeric(user_id)){ 
            return;
        }
        if(vote_value > 1 || vote_value < 0){
            return;
        }

        // SHOW THE PROCESSING DIV
        this.show_processing(comment_id);

        voterObj = this;

        // AJAX CALL
        new AJAX.FileRequest(
            '/ajax/comment_spam.php',
            this.spam_result,
            null,
            'post',
            'comment_id='+comment_id+'&vote_value='+vote_value+'&user_id='+user_id
        );
    },

    spam_result : function() {
        if(this.request && this.response){

            // CAPTURE THE RESPONSE
            var the_response = this.response; 
            //alert(this.response);

            // ATTEMPT TO PARSE RESPONSE
            var parsed =  JSON.parse(the_response);

            // PARSE VALIDATION
            if(!parsed){
                //alert(this.response);                 
                delete voterObj;
                return;
            }

            // MESSAGING
            switch(parsed['message_type']){
                case 0:
                    voterObj.respond(parsed['comment_id'], parsed['message_type'], parsed['message'], parsed['up'], parsed['down']);
                    break;
                case 1:
                    voterObj.parse_spam_json(parsed);
                    voterObj.respond(parsed['comment_id'], parsed['message_type'], parsed['message'], parsed['thumbs_up'], parsed['thumbs_down']);
                    break;

                // IMPROPER I/O
                default:
                    delete voterObj;
                    return;
                    break;
            }
        }
        delete voterObj;
    },

    result: function() {

        if(this.request && this.response){

            // CAPTURE THE RESPONSE
            var the_response = this.response; 

            // ATTEMPT TO PARSE RESPONSE
            var parsed =  JSON.parse(the_response);

            // PARSE VALIDATION
            if(!parsed){ 
                // FAILED TO PARSE
                delete voterObj;
                return;
            }

            // IF THIS IS THE SPAM RESPONSE AND THE RESPONSE IS OF TYPE SUCCESS...
            if(parsed['spam'] && parsed['message_type'] == 1){
                // SWITCH THE ICONS
                if(parsed['spam'] == 0){
                    voterObj.show('is_spam_'+parsed['comment_id']);
                    voterObj.hide('not_spam_'+parsed['comment_id']);
                }else{
                    voterObj.show('not_spam_'+parsed['comment_id']);
                    voterObj.hide('is_spam_'+parsed['comment_id']);
                }
            }

            // SOME OUTPUT VALIDATION
            if(
                isBlank(parsed['message']) || 
                !isNumeric(parsed['message_type']) || 
                !isNumeric(parsed['comment_id']) ||
                !isNumeric(parsed['up']) ||
                !isNumeric(parsed['down'])
            ){ 
                // FAILED TO MAKE IT THROUGH THE I/O VALIDATION
                //alert('problem'); 
            }

            // MESSAGING
            switch(parsed['message_type']){

                case 0:
                case 1:
                    voterObj.respond(parsed['comment_id'], parsed['message_type'], parsed['message'], parsed['up'], parsed['down']);
                    break;

                // IMPROPER I/O
                default:
                    delete voterObj;
                    return;
                    break;
            }
        }
        delete voterObj;
    },

    comment_must_log_in:function(bit,comment_id) {
        this.respond(comment_id,0,'You must log in to approve, disapprove or mark a comment as spam.<br>  <a href="/login" title="login">Click here to login</a> or take a moment to <a href="/quickregister" title="Register on AdAge.com">register</a>.');
    },

    respond:function(comment_id, response_type, message, up, down){
        
        // HIDE THE PROCESSING DIV
        this.hide_processing(comment_id);

        switch(response_type){

            case 1:
                // SETTING THE SUCCESS MESSAGE
                var the_div = document.getElementById('comment_feedback_'+comment_id);
                the_div.className = 'comment_vote_success';
                the_div.innerHTML = message;

                // COMMENT VOTING COUNTS
                var comments_up = document.getElementById('thumbs_up_'+comment_id);
                var comments_down = document.getElementById('thumbs_down_'+comment_id);
                if(comments_up && comments_down){
                    comments_up.innerHTML = (up == 0)?'':'+'+up;
                    comments_down.innerHTML = (down == 0)?'':'-'+down;
                }
                break;

            case 0:
                // SETTING THE ERROR MESSAGE
                var the_div = document.getElementById('comment_feedback_'+comment_id);
                the_div.className = 'comment_vote_error';
                the_div.innerHTML = message;

                break;

            default:
                // THIS IS A PRETTY AMAZING ERROR
                return;
                break;
        }
    },

    show_processing:function(id) {
        document.getElementById('processing_'+id).innerHTML = '<img src="/images/global/ajax-loader-003.gif" height="16" width="16"/>';
    },

    hide_processing:function(id){
        document.getElementById('processing_'+id).innerHTML = '';
    },

    show:function(id, mode){
        if(!mode){ mode = 'show'; }
        var the_object = document.getElementById(id);
        if(the_object && mode == 'show'){
            the_object.style.display = 'block';
            the_object.style.visibility = 'visible';
        }else if(the_object){
            the_object.style.display = 'none';
            the_object.style.visibility = 'hidden';
        }
    },

    hide:function(id){
        this.show(id,'hide');
    },

    parse_spam_json:function(json) {

        // THE DIV TARGET
        var the_target = document.getElementById('comment_container_id_'+json.comment_id);
        document.getElementById('voting_links_'+json.comment_id).innerHTML = '';

		if (!comment_container_ids[json.comment_id]) {
			comment_container_ids[json.comment_id] = the_target.innerHTML;
		}

        // WHAT WE WILL BE BUILDING NAD USING AS A REPLACEMENT, ULTIMATELTY
        var the_output = '';

        // VOTING BLOCK
        var voting_links_html = '';

        // LOGIC THAT IS INCLUDED 
        var attribution = (isEmail(json.username) && json.firstName != "")?json.firstName:json.username;
        var state = (trim(json.state) != "")?', '+json.state.toUpperCase():'';
        var viewer_id = (trim(json.user_id) != "")?json.user_id:-1;

        var thumbs_up   = (json.thumbs_up == 0)?'':'+'+json.thumbs_up;
        var thumbs_down = (json.thumbs_down == 0)?'':'-'+json.thumbs_down;

        // BUILD THE ACTUAL CONTENTS - START WITH WETHER OR NOT THE ITEM HAS BEEN MARKED AS SPAM
        if(json.spam == 0){

            // BUILD THE VOTING BLOCK
            var voting_html = [];

            // IF THE USER IS LOGGED IN 
            if(viewer_id != -1){
                if(json.spam == 1){
                    voting_html.push('<div id="not_spam_',json.comment_id,'" class="not_spam" title="Not spam" onclick="javascript:Voter.vote_spam(0,',json.comment_id,',',viewer_id,');"><\/div>');
                    voting_html.push('<div id="is_spam_',json.comment_id,'" class="collapse_spam" title="Hide spam" onclick="javascript:Voter.vote_spam(1,',json.comment_id,',',viewer_id,');" style="display: none; visibility: hidden;"><\/div>');
                }else{
                    voting_html.push('<div id="not_spam_',json.comment_id,'" class="not_spam" title="Not spam" onclick="javascript:Voter.vote_spam(0,',json.comment_id,',',viewer_id,');"  style="display: none; visibility: hidden;"><\/div>');
                    voting_html.push('<div id="is_spam_',json.comment_id,'" class="collapse_spam" title="Hide spam" onclick="javascript:Voter.vote_spam(1,',json.comment_id,',',viewer_id,');"><\/div>');
                }
                voting_html.push('<div class="vote_down" title="Vote down" onclick="javascript:Voter.vote_comment(0,',json.comment_id,',',viewer_id,');" ><\/div><div class="numeric down" id="thumbs_down_', json.comment_id, '">', thumbs_down,'<\/div>');
                voting_html.push('<div class="vote_up" title="Vote up" onclick="javascript:Voter.vote_comment(1,',json.comment_id,',',viewer_id,');" ><\/div><div class="numeric up" id="thumbs_up_', json.comment_id, '">', thumbs_up, '<\/div>');
                
            }else{
                if(json.spam == 0){
                    voting_html.push('<div class="not_spam" title="Not spam" onclick="javascript: Voter.comment_must_log_in(', json.comment_id,');"><\/div>');
                }else{
                    voting_html.push('<div class="collapse_spam" title="Hide spam" onclick="javascript: Voter.comment_must_log_in(', json.comment_id,');"><\/div>');
                }
                voting_html.push('<div class="vote_down" title="Vote down" onclick="javascript: Voter.comment_must_log_in(', json.comment_id,');"><\/div><div class="numeric down" id="thumbs_down_', json.comment_id,'">', thumbs_down, '<\/div>');
                voting_html.push('<div class="vote_up" title="Vote up" onclick="javascript:Voter.comment_must_log_in(', json.comment_id,');" ><\/div><div class="numeric up" id="thumbs_up_', json.comment_id,'">', thumbs_up, '<\/div>');
            }
            voting_html.push('<div class="processing" id="processing_', json.comment_id,'"><img src="/images/global/ajax-loader-003.gif" height="16" width="16" ><\/div>');
            voting_links_html = voting_html.join('');

            // BUILD THE HTML OUTPUT
            var html = [];
            html.push('<div class="commentor" style="clear:left">');
            html.push(comment_container_ids[json.comment_id]);
            html.push('<div align="left" class="voting_links">');
            html.push(voting_links_html);
            html.push('<div id="comment_feedback_',json.comment_id,'"><\/div>');
            html.push('<\/div><\/div>');
            the_output = html.join('');

        // SPAM MARKING
        }else{

            // BUILD THE VOTING BLOCK
            var voting_html = [];

            // IF THE USER IS LOGGED IN 
            if(viewer_id != -1){
                voting_html.push('<div id="not_spam_',json.comment_id,'" class="not_spam" title="Not spam" onclick="javascript:Voter.vote_spam(0,',json.comment_id,',',viewer_id,');"><\/div>');
                voting_html.push('<div id="is_spam_',json.comment_id,'" class="collapse_spam" title="Hide spam" onclick="javascript:Voter.vote_spam(1,',json.comment_id,',',viewer_id,');" style="display: none; visibility: hidden;"><\/div>');
			}else{
				voting_html.push('<div id="not_spam_',json.comment_id,'" class="not_spam" title="Not spam" onclick="javascript: Voter.comment_must_log_in(',json.comment_id,');" ><\/div>');
			}

            voting_html.push('<div class="processing" id="processing_', json.comment_id,'"><img src="/images/global/ajax-loader-003.gif" height="16" width="16" /><\/div>');
            voting_links_html = voting_html.join('');

            // BUILD THE HTML OUTPUT
            var html = [];
            html.push('<div class="spam_message" style="display:block;">');
            html.push('<p>Comment marked as spam <a href="javascript:Voter.toggle(\'spam_message_',json.comment_id,'\',\'show_hide\');" id="show_hide">show</a><\/p>');
            html.push('<\/div>');
            html.push('<div class="row_comments" id="spam_message_', json.comment_id, '" style="display:none;">');
            html.push(comment_container_ids[json.comment_id]);
            html.push('<div align="left" class="voting_links">');
            html.push(voting_links_html);       
            html.push('<div id="comment_feedback_',json.comment_id,'"><\/div>');
            html.push('<\/div><\/div>');            
            the_output = html.join(''); 
        }

        // REPLACE THE EXISTING
        the_target.innerHTML = the_output;

        // HIDE THE SPINNER
        this.hide_processing(json.comment_id);
    },

    toggle:function(comment, link){
        if (document.getElementById(comment).style.display == "none") {
            document.getElementById(comment).style.display = "block";
            document.getElementById(link).innerHTML = "hide";
        } else {
            document.getElementById(comment).style.display = "none";
            document.getElementById(link).innerHTML = "show";
        }
    },

    // SET THE COOKIE
    setSortCookie:function(argument){
        var value = 'date';
        if(argument == 'pop'){
            value = 'pop';
        }
        document.cookie = "adage_comment_sort=" +escape(value);
    }
};
</script>

