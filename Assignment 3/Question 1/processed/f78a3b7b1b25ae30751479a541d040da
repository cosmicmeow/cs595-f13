<script type="text/javascript">

var LookBook = {
	count : 0,
	companies : [],
	save_company : function(obj) {
		if(obj !== undefined && this.company_saved(obj.agency) == false) {
			this.companies[this.count] = obj;
			this.count++;
		}
	},

	company_saved : function(str) {
		for(var j=0; j<=this.companies.length; j++) {
			if(this.companies[j] !== undefined && this.companies[j].agency !== undefined && this.companies[j].agency.toLowerCase() == str.toLowerCase()) {
				return true;
			}
		}
		return false;
	},

	output_companies : function() {
		if(this.companies.length == 0) {
			$("#spotlight").css("display","none");
		}
		else {
			// loop through premium listings
			for(var j=0; j<=this.companies.length; j++) {
				if(this.companies[j] !== undefined && this.companies[j].premium) {

					var target = "lbp"+ j;
					var obj = this.companies[j];

					//tab containers
					$("#lbholder").append('<div class="tab" id="'+target+'"></div>');

					//tab nav
					$("div[id*="+target+"]").append('<input type="radio" id="tab-'+target+'" name="tab-group"><label class="premium_listing" for="tab-'+target+'">'+obj.agency+'</label>');

					//format companies
					$("div[id*="+target+"]").append('<div class="content"><span class="sixcol spotlight_left"><span class="pad"><img src="http://lookbook.adage.com/'+obj.image_filepath+'" id="image_'+target+'" class="spotlight_logo" /><ul class="creds"><li><a href="http://lookbook.adage.com/'+obj.url+'"" onclick="trackOutboundLink(this, \'LookBook-Spotlight\',\''+obj.agency.replace(/''/g,"\'") + '\', this.getAttribute(\'href\')); return false;">'+obj.agency+'<\/a></li><li>EMPLOYEES:</li><li class="employees">'+obj.employees+'</li></ul><ul id="special_'+target+'" class="special"></ul></span></span><span class="sixcol spotlight_right last"><span class="pad"><h3>about '+obj.agency+'</h3><p class="about">'+obj.summary+'</p><p><b><a href="http://lookbook.adage.com/'+obj.url+'" onclick="trackOutboundLink(this, \'LookBook-Spotlight\',\''+obj.agency.replace(/''/g,"\'") + '\', this.getAttribute(\'href\')); return false;">Learn more about '+obj.agency+'<\/a></b></p></span></span></div>');

					//specialties
					if(obj.specialties.length > 0) {

					$('#special_'+target).append('<li>specialties:</li>');

						for(var n=0; n<=obj.specialties.length; n++) {
							if(obj.specialties[n] !== undefined && obj.specialties[n] != false) {
								$('#special_'+target).append('<li>'+obj.specialties[n]+'</li>');	
							}
						}
					}

					//null image catcher
					if (obj.image_filepath == null ){
						$('#image_'+target).attr("src","http://gaia.adage.com/images/lookbook_default.png");
					}
				}
			}

			// loop through basic listings
			for(var j=0; j<=this.companies.length; j++) {
				if(this.companies[j] !== undefined && !this.companies[j].premium) {

					var target = "lbb"+ j;
					var obj = this.companies[j];

					//tab containers
					$("#lbholder").append('<div class="tab" id="'+target+'"></div>');

					//tab nav
					$("div[id*="+target+"]").append('<input type="radio" id="tab-'+target+'" name="tab-group"><label class="basic_listing" for="tab-'+target+'">'+obj.agency+'</label>');

					//format companies
					$("div[id*="+target+"]").append('<div class="content"><span class="twelvecol spotlight_left"><span class="pad"><img src="http://lookbook.adage.com/'+obj.image_filepath+'" id="image_'+target+'" class="spotlight_logo" /><ul class="creds"><li><a href="http://lookbook.adage.com/'+obj.url+'" onclick="trackOutboundLink(this, \'LookBook-Spotlight\',\''+obj.agency.replace(/''/g,"\'") + '\',\'http://lookbook.adage.com/'+obj.url+'\'); return false;">'+obj.agency+'</a></li></ul><p>For more Information on '+obj.agency+' and other companies like this, visit LookBook-- a directory for any marketing industry professional seeking new partners.</p><p><b><a href="http://lookbook.adage.com">Learn more about LookBook<\/a></b></p></span></span></span></div>');

					//null catcher
					if (obj.image_filepath == null ){
						$('#image_'+target).attr("src","http://gaia.adage.com/images/lookbook_default.png");
					}

					//init checkbox
					$('.tab:nth-child(1) input').attr('checked', true);	
				}
			}
		}
	}
};

$(document).ready(function() {

	// just loop through links in the article
	var current_article = document.getElementById('article_body');
	var story_links = current_article.getElementsByTagName('a');

	// loop through all directory links, and store unique LookBook results
	for(var i=0; i<=story_links.length; i++) {

		if(story_links[i] !== undefined && story_links[i].className == 'directory_entry') {

			// don't perform ajax request if company already saved
			if(LookBook.company_saved(story_links[i].innerHTML) == false) {
				$.ajax({
					url: "/api/lookbook/listing.php",
					type: "GET",
					async: false,
					data: ({'q': story_links[i].innerHTML}),
					success: function(data) {

						// results in JSON format
						var obj = jQuery.parseJSON(data);
						if(obj !== undefined && obj.agency !== undefined && LookBook.company_saved(obj.agency) == false) {
							LookBook.save_company(obj);
						}

						//turn on
						$("#spotlight").css("display","block");

					}//end success
				});
			}
		}
	}
	LookBook.output_companies();

});//onload

</script>

