<script type="text/javascript" language="Javascript">

function nice_newsletter(f) {

	// FUNCTION VARIABLES
	var error = 0;

	// CLEAR PREVIOUS SUBMISSION ERRORS
	clearErrors();
	// FORM HARVESTING
	var email = f.nl_email.value;

	// VALIDATION
	if(!isEmail(email.trim())) {
		document.getElementById('signup_error').innerHTML = '* Please enter a valid email address.';
		error = 1;
	}	
	if(error == 1) {
		return false;
	}
	try {
		_gaq.push(['_trackEvent', 'Modules', 'Ad Age Media Buzz Newsletter Widget', '/article/media/netflix-seeks-presence-u-s-cable-tv-systems-expand/244395/']); 
	} catch(err){}

	// ALL GOOD SHOW THANK YOU 
	showThankYou();

	setTimeout(function() {
		console.log('tracking foobar');
        f.submit();
	}, 1000);

	// SUBMIT POST FORM
	return false;
}

function clearErrors() {
	document.getElementById('signup_error').innerHTML = '';
}

function showThankYou() {
	document.getElementById('nl_email').style.visibility = 'hidden';
	document.getElementById('nl_email').style.display = 'none';
	document.getElementById('msg').innerHTML = 'Thank you for signing up.';
}

</script>

